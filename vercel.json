{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/public/$1"
      },
      {
        "src": "/(.*\\.(js|css|png|jpg|gif|svg|ico)$)",
        "dest": "dist/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dist/public/index.html"
      }
    ]
  }